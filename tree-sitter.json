{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "icalendar",
      "camelcase": "iCalendar",
      "title": "iCalendar",
      "scope": "source.icalendar",
      "file-types": ["icalendar", "ical", "ics", "ifb"],
      "injection-regex": "^icalendar$",
      "class-name": "TreeSitterIcalendar"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MPL-2.0",
    "description": "Icalendar grammar for tree-sitter",
    "authors": [
      {
        "name": "Edwin Kofler",
        "email": "edwin@kofler.dev",
        "url": "https://edwinkofler.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/hyperupcall-experiments/tree-sitter-icalendar"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
